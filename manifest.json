{
  "manifest_version": 3,
  "name": "New Toby - Tab Manager",
  "version": "1.0",
  "description": "A clean and efficient tab management extension",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
